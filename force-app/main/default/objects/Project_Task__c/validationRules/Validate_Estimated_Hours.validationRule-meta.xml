<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_Estimated_Hours</fullName>
    <active>true</active>
    <description>Validates Estimated_Hours__c is greater than or equal to 0</description>
    <errorConditionFormula>Estimated_Hours__c &lt; 0</errorConditionFormula>
    <errorDisplayField>Estimated_Hours__c</errorDisplayField>
    <errorMessage>Estimated Hours cannot be negative.</errorMessage>
</ValidationRule>
